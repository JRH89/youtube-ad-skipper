{
    "manifest_version": 3,
    "name": "no-ads",
    "version": "1.1",
    "description": "Ad skipper for YouTube. This extension will automatically skip ads by clicking the skip button when it appears.",
    "permissions": [],
    "host_permissions": [
        "*://*.youtube.com/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://*.youtube.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}